{
  "name": "spring-integration-progress-bar",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "src/main/typescript",
    "src/test/typescript"
  ],
  "scripts": {
    "test": "npm run test --workspace=src/test/typescript",
    "dev": "npm run dev --workspace=src/main/typescript",
    "dev:codespaces": "npm run dev:codespaces --workspace=src/main/typescript",
    "build": "npm run build --workspace=src/main/typescript",
    "preview": "npm run preview --workspace=src/main/typescript",
    "check": "npm run check --tsconfig src/main/typescript/tsconfig.json"
  }
}
